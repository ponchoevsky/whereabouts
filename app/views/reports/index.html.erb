<div class="row">
  <% @reports.each do |report| %>
    <div class="col-sm-6 col-md-3">
      <div class="thumbnail">
        <%= link_to( image_tag(report.image.url(:medium), :class => "image"), report) %>
        <center><h3 style="display: inline"><%=report.name%></h3> (<%= report.city %>)</center>
      </div>
    </div>
  <% end %>
</div>